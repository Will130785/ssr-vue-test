{
  "name": "ssr-vue-test",
  "version": "1.0.0",
  "description": "Vue.js & Vue-Server-Renderer",
  "scripts": {
    "dev": "rimraf dist && cross-env NODE_ENV=development node server.js",
    "production": "rimraf dist && npm run build && npm run prod-server",
    "start": "npm run build && npm run start-server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js --progress",
    "start-server": "cross-env node server.js",
    "prod-server": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "axios": "^0.21.1",
    "cross-env": "^5.1.3",
    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "vue": "^2.6.14",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "chokidar": "^2.1.8",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "rimraf": "^3.0.2",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}

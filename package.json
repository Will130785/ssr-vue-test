{
  "name": "ssr-vue-test",
  "version": "1.0.0",
  "description": "Vue.js & Vue-Server-Renderer",
  "scripts": {
    "dev": "rimraf dist && cross-env NODE_ENV=development node server.js",
    "production": "rimraf dist && npm run build && npm run prod-server",
    "start": "npm run build && npm run start-server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js --progress",
    "start-server": "cross-env node server.js",
    "prod-server": "cross-env NODE_ENV=production PORT=5000 node server.js",
    "dev-test": "webpack serve"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^5.1.3",
    "es6-promise": "^4.2.4",
    "express": "^4.17.1",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^3.0.1",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/helper-compilation-targets": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^8.2.2",
    "chokidar": "^2.1.8",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.1",
    "postcss-loader": "^6.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
